{
  "kind": "collectionType",
  "collectionName": "forms",
  "info": {
    "singularName": "form",
    "pluralName": "forms",
    "displayName": "CONTRACTS: Form",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "tenant": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::tenant.tenant"
    },
    "name": {
      "type": "string"
    },
    "link": {
      "type": "string"
    },
    "formId": {
      "type": "string"
    },
    "uuid": {
      "type": "customField",
      "regex": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$",
      "customField": "plugin::field-uuid.uuid"
    },
    "publicForm": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::public-form.public-form"
    },
    "sendTo": {
      "type": "email"
    },
    "contact": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::contact.contact"
    }
  }
}
