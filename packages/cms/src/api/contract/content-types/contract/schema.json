{
  "kind": "collectionType",
  "collectionName": "contracts",
  "info": {
    "singularName": "contract",
    "pluralName": "contracts",
    "displayName": "CONTRACTS: Contract",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "contractId": {
      "type": "string"
    },
    "uuid": {
      "type": "customField",
      "regex": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$",
      "customField": "plugin::field-uuid.uuid"
    },
    "link": {
      "type": "string"
    },
    "tenant": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::tenant.tenant"
    },
    "publicContract": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::public-contract.public-contract"
    },
    "contact": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::contact.contact"
    }
  }
}
