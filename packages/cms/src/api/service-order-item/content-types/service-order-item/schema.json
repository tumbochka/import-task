{
  "kind": "collectionType",
  "collectionName": "service_order_items",
  "info": {
    "singularName": "service-order-item",
    "pluralName": "service-order-items",
    "displayName": "SELLING: Service Order Item",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "quantity": {
      "type": "integer"
    },
    "note": {
      "type": "text"
    },
    "purchaseType": {
      "type": "enumeration",
      "enum": ["buy", "rent"]
    },
    "order": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::order.order",
      "inversedBy": "services"
    },
    "service": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::service-performer.service-performer",
      "inversedBy": "selling_service_order_items"
    },
    "itemId": {
      "type": "string",
      "required": true,
      "regex": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$"
    },
    "price": {
      "type": "decimal"
    },
    "discounts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::discount.discount"
    },
    "tax": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::tax.tax"
    },
    "status": {
      "type": "enumeration",
      "enum": ["draft", "published"],
      "default": "published"
    },
    "dueDate": {
      "type": "date"
    },
    "isShowInvoiceNote": {
      "type": "boolean",
      "default": true
    },
    "isVisibleInDocs": {
      "type": "boolean",
      "default": true
    },
    "taxCollection": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::tax-collection.tax-collection"
    },
    "files": {
      "allowedTypes": ["images", "files", "videos", "audios"],
      "type": "media",
      "multiple": true
    }
  }
}
