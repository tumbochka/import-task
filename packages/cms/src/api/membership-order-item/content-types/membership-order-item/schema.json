{
  "kind": "collectionType",
  "collectionName": "membership_order_items",
  "info": {
    "singularName": "membership-order-item",
    "pluralName": "membership-order-items",
    "displayName": "SELLING: Membership Order Item",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "quantity": {
      "type": "integer"
    },
    "note": {
      "type": "text"
    },
    "membership": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::membership.membership",
      "inversedBy": "membershipOrderItems"
    },
    "order": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::order.order",
      "inversedBy": "memberships"
    },
    "purchaseType": {
      "type": "enumeration",
      "enum": [
        "buy",
        "rent"
      ]
    },
    "itemId": {
      "type": "string",
      "required": true,
      "regex": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$"
    },
    "price": {
      "type": "decimal"
    },
    "discounts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::discount.discount"
    },
    "tax": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::tax.tax"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "draft",
        "published"
      ],
      "default": "published"
    },
    "isShowInvoiceNote": {
      "type": "boolean",
      "default": true
    },
    "isVisibleInDocs": {
      "type": "boolean",
      "default": true
    },
    "taxCollection": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::tax-collection.tax-collection"
    }
  }
}
