      "type": "relation",
      "relation": "oneToMany",
      "target": "api::ecommerce-custom-app-service.ecommerce-custom-app-service",
      "mappedBy": "tenant"
    },
    "status": {
      "type": "boolean",
      "default": true
    },
    "paymentMethods": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::payment-method.payment-method",
      "mappedBy": "tenant"
    },
    "emailSender": {
      "type": "string",
      "default": "no-reply"
    },
    "storageUsage": {
      "type": "string"
    },
    "stripe_subscription_plans": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::stripe-subscription-plan.stripe-subscription-plan",
