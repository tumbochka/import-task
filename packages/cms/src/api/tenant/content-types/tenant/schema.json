    },
    "selling_orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "tenant"
    },
    "dealTransactionReminders": {
      "type": "relation",
      "relation": "oneToMany",
