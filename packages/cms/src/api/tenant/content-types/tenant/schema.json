    "rechargeToBalance": {
      "type": "decimal",
      "min": 0
    },
    "autoRechargeCredit": {
      "type": "boolean",
      "default": false
    },
    "onboarding": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::onboarding.onboarding",
      "mappedBy": "tenant"
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "mainLocation": {
      "type": "relation",
      "relation": "oneToOne",
